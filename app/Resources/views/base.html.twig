<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}ZSTeaser{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('bundles/app/plugins/pace/pace-theme-flash.css') }}" rel="stylesheet" type="text/css" media="screen"/>
            <link href="{{ asset('bundles/app/plugins/boostrapv3/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('bundles/app/plugins/boostrapv3/css/bootstrap-theme.min.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('bundles/app/plugins/font-awesome/css/font-awesome.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('bundles/app/css/animate.min.css') }}" rel="stylesheet" type="text/css"/>

            <link href="{{ asset('bundles/app/css/style.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('bundles/app/css/responsive.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('bundles/app/css/magic_space.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('bundles/app/css/custom-icon-set.css') }}" rel="stylesheet" type="text/css"/>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body class="{% block bodyClass %}{% endblock %}">
        {% block body %}{% endblock %}

        {% block javascripts %}
            <script src="{{ asset('bundles/app/plugins/jquery-1.9.1.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/app/plugins/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/app/plugins/pace/pace.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/app/plugins/jquery-validation/js/jquery.validate.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/app/plugins/jquery-lazyload/jquery.lazyload.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/app/js/login_v2.js" type="text/javascript') }}"></script>
            <script src="{{ asset('bundles/app/js/core.js') }}" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>
